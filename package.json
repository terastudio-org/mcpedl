{
  "name": "improved-mcpedl-api",
  "version": "2.0.0",
  "description": "Unofficial MCPEDL Node.js API - Improved and Enhanced",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "prepare": "npm run build",
    "example": "ts-node examples/usage.ts"
  },
  "keywords": [
    "mcpedl",
    "minecraft",
    "api",
    "scraping",
    "nodejs",
    "typescript"
  ],
  "author": "MiniMax Agent",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "p-retry": "^6.0.0",
    "bottleneck": "^2.19.5"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/jest": "^29.0.0",
    "typescript": "^5.0.0",
    "ts-node": "^10.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/terastudio-org/mcpedl.git"
  },
  "bugs": {
    "url": "https://github.com/terastudio-org/mcpedl/issues"
  },
  "homepage": "https://github.com/terastudio-org/mcpedl#readme"
}